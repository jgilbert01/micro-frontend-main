{
  "name": "@mfe/tasks",
  "scripts": {
    "start": "webpack serve --port 9004",
    "start:standalone": "webpack serve --port 9001 --env standalone",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext js",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "dp:stg:e": "eval \"$(assume-role) s3-deploy './dist/**' --cwd './dist/' --region us-east-1 --bucket $STG_E_BUCKET_NAME --cache 31536000 --private\"",
    "dp:stg:w": "eval \"$(assume-role) s3-deploy './dist/**' --cwd './dist/' --region us-west-1 --bucket $STG_W_BUCKET_NAME --cache 31536000 --private\"",
    "dp:prd:e": "eval \"$(assume-role) s3-deploy './dist/**' --cwd './dist/' --region us-east-1 --bucket $PRD_E_BUCKET_NAME --cache 31536000 --private\"",
    "dp:prd:w": "eval \"$(assume-role) s3-deploy './dist/**' --cwd './dist/' --region us-west-1 --bucket $PRD_W_BUCKET_NAME --cache 31536000 --private\""
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.16",
    "@types/systemjs": "^6.1.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-jest": "^26.6.3",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-important-stuff": "^3.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "s3-deploy": "^1.4.0",
    "single-spa-react": "^3.2.0",
    "webpack": "^5.8.0",
    "webpack-cli": "^4.2.0",
    "webpack-config-single-spa-react": "^2.0.0-beta.1",
    "webpack-dev-server": "^4.0.0-beta.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@reach/router": "^1.3.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.2"
  }
}
