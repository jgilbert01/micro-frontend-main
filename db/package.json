    "react-hook-form": "^7.31.0",
    "react-icons": "^4.10.1",
    "react-query": "^3.39.1",
    "react-router-dom": "6.3.0",
    "single-spa-react": "^4.6.0",
    "zod": "^3.17.3"
  }
}
